{
    "name": "zabrdast/livewire-storefront",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yasir Turk",
            "email": "yasirmturk@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "livewire/livewire": "^2.9",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Zabrdast\\Store\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zabrdast\\Store\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": "Zabrdast\\Store\\ZabrdastServiceProvider"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
